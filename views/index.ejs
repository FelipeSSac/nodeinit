<body>
  <div id="navbar-full">
    <div class="container">
      <nav class="navbar navbar-ct-blue navbar-transparent navbar-fixed-top" role="navigation">
        <div class="container">
          <!-- Brand and toggle get grouped for better mobile display -->
          <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse"
              data-target="#bs-example-navbar-collapse-1">
              <span class="sr-only">Toggle navigation</span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
            </button>
            <a href="http://creative-tim.com">
              <div class="logo-container">
                <div class="logo">
                  <img src="assets/img/user.png">
                </div>
                <div class="brand">
                  <%= title %>
                </div>
              </div>
            </a>
          </div>
          <!-- Collect the nav links, forms, and other content for toggling -->
          <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
            <ul class="nav navbar-nav navbar-right">
              <li><a href="components.html">Components</a></li>
              <li><a href="http://www.creative-tim.com/product/get-shit-done-kit"
                  class="btn btn-round btn-default">Download</a></li>
            </ul>

          </div><!-- /.navbar-collapse -->
        </div><!-- /.container-fluid -->
      </nav>
    </div><!--  end container-->
    <div class='blurred-container'>
      <div class="motto">
        <div class="border" id="main--middle-text">
          Force
        </div>
        <div>Side</div>
      </div>
      <div id="bg" class="img-src" style="background-image: url('assets/img/bg-sw.jpg')"></div>
    </div>
  </div>
  <div class="main">
    <div class="container tim-container">
      <div class="tim-title">
        <h2>Layout Bonito</h2>
      </div>
      <!-- end buttons div -->
      <div id="inputs">
        <div class="row">
          <div class="col-sm-6">
            <div class="tim-title">
              <h3>Come to the ...</h3>
            </div>
            <label class="radio ct-red">
              <input type="radio" name="optionsRadios" data-toggle="radio" id="radioA">
              <i></i>Dark Side
            </label>
            <label class="radio">
              <input type="radio" name="optionsRadios" data-toggle="radio" id="radioB">
              <i></i>Bright Side
            </label>
          </div>
          <div class="col-sm-6">
            <div class="tim-title">
              <h3>Masters:</h3>
            </div>
            <div class="col-sm-3 dropdown">
              <button class="btn dropdown-toggle  btn-danger" type="button" id="dropdownMenu1" data-toggle="dropdown">
                <%= NameA %>
                  <span class="caret"></span>
              </button>
              <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
                <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Lord Darth Vader</a></li>
                <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Lord Darth Maul</a></li>
                <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Lord Darth Tyranus</a></li>
                <li role="presentation" class="divider"></li>
                <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Lord Darth Sidious</a></li>
              </ul>
            </div>
            <div class="col-sm-3 dropdown">
              <button class="btn dropdown-toggle btn-primary" type="button" id="dropdownMenu1" data-toggle="dropdown">
                <%= NameB %>
                  <span class="caret"></span>
              </button>
              <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
                <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Jedi Master Luke Skywalker</a></li>
                <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Jedi Master Obi-Wan Kenoby</a></li>
                <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Jedi Master Qui-Gon Jinn</a></li>
                <li role="presentation" class="divider"></li>
                <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Jedi Master Yoda</a></li>
              </ul>
            </div>
          </div>
        </div><!--  end row -->
      </div><!--  end inputs -->
      <div id="footer">
        <div class="row">
          <div class="col-12">
            <p>Por Felipe dos Santos Souza</p>
          </div>
        </div>
      </div>
    </div>
  </div>
</body>

<script src="jquery/jquery-1.10.2.js" type="text/javascript"></script>
<script src="assets/js/jquery-ui-1.10.4.custom.min.js" type="text/javascript"></script>
<script src="bootstrap3/js/bootstrap.js" type="text/javascript"></script>
<script src="assets/js/gsdk-checkbox.js"></script>
<script src="assets/js/gsdk-radio.js"></script>
<script src="assets/js/gsdk-bootstrapswitch.js"></script>
<script src="assets/js/get-shit-done.js"></script>
<script src="assets/js/custom.js"></script>
<script type="text/javascript">

  $('.btn-tooltip').tooltip();
  $('.label-tooltip').tooltip();
  $('.pick-class-label').click(function () {
    var new_class = $(this).attr('new-class');
    var old_class = $('#display-buttons').attr('data-class');
    var display_div = $('#display-buttons');
    if (display_div.length) {
      var display_buttons = display_div.find('.btn');
      display_buttons.removeClass(old_class);
      display_buttons.addClass(new_class);
      display_div.attr('data-class', new_class);
    }
  });
  $("#slider-range").slider({
    range: true,
    min: 0,
    max: 500,
    values: [75, 300],
  });
  $("#slider-default").slider({
    value: 70,
    orientation: "horizontal",
    range: "min",
    animate: true
  });
  $('.carousel').carousel({
    interval: 4000
  });

  $('#radioA').on('toggle', function () {
    $('#sw-name').html('<%= NameA %>');
    $('#main--middle-text').animate({ 'opacity': 0 }, 400, function () {
      $(this).html('<%= SideA %>').animate({ opacity: 1 }, 400).addClass('dark-side--border').removeClass('bright-side--border').removeClass('border');
    });
    $('#bg').animate({ height: '100vh' }, 400, function () {
      $(this).css({ 'background-position-x': 'left' })
    });
  });

  $('#radioB').on('toggle', function () {
    $('#sw-name').html('<%= NameB %>');
    $('#main--middle-text').animate({ 'opacity': 0 }, 400, function () {
      $(this).html('<%= SideB %>').animate({ 'opacity': 1 }, 400).addClass('bright-side--border').removeClass('dark-side--border').removeClass('border');
    });
    $('#bg').animate({ height: '100vh' }, 400, function () {
      $(this).css({ 'background-position-x': 'right' })
    });
  });


</script>

</html>